  "type": "module",
  "scripts": {
    "prod": "npm run production",
    "production": "npm run build",
    "dev": "gulp & nuxt --spa --port=3039 --hostname=localhost",
    "build": "gulp once && nuxt build --spa",
    "start": "nuxt start --spa --port=3039 --hostname=localhost",
    "generate": "gulp once && nuxt generate",
    "analyze": "gulp once && nuxt build --spa --analyze",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "lint:staged": "eslint $(git diff --name-only HEAD --diff-filter=d | grep -E '\\.(js|vue)$' | xargs) --ignore-path .gitignore",
    "lint:staged-fix": "eslint $(git diff --name-only HEAD --diff-filter=d | grep -E '\\.(js|vue)$' | xargs) --ignore-path .gitignore --fix",
    "swagger:portfolio": "curl 'https://api.swaggerhub.com/apis/KLIM0VSERGEY/honee_portfolio' |  ./node_modules/node-jq/bin/jq '.apis|last|.properties[0].url' | xargs -I {} ./node_modules/.bin/swagger-typescript-api -p '{}' --no-client -o api -n portfolio-models.d.ts",
    "precommit": "echo 'Pre-commit checks...' && npm run lint:staged",
    "test": "node --experimental-vm-modules -r dotenv/config node_modules/.bin/jest"
  },
  "pre-commit": [
    "precommit"
  ],
